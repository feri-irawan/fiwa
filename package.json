{
  "name": "fiwa",
  "description": "A lightweight WhatsApp bot with built-in MongoDB integration.",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "bun": "./dist/esm/index.js",
    "node": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "default": "./dist/esm/index.js",
    "types": "./dist/types/index.d.ts"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "bun run build:esm && bun run build:cjs"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "baileys": "^6.7.16",
    "mongodb": "^6.15.0",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "jimp": "^1.6.0",
    "link-preview-js": "^3.0.14",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.34.1",
    "typescript": "^5"
  },
  "author": "Feri Irawan",
  "homepage": "https://github.com/feri-irawan/fiwa#readme",
  "repository": "https://github.com/feri-irawan/fiwa",
  "bugs": "https://github.com/feri-irawan/fiwa/issues",
  "license": "MIT",
  "keywords": [
    "whatsapp",
    "baileys",
    "mongodb",
    "typescript",
    "automation",
    "messaging",
    "js-whatsapp",
    "whatsapp-api",
    "whatsapp-web",
    "whatsapp-chat",
    "whatsapp-group",
    "multi-device"
  ]
}
